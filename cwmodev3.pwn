			format(logmessage, sizeof(logmessage), "{FFC600}[RPM]{bdbdbd} %s{9e9e9e} > {bdbdbd}%s{9e9e9e}: %s", sendername, targetname, message);
			SendClientMessage(i, -1, logmessage); // ùåìç àú ääåãòä ìàãîéðéí
		}
	}
}
