	    if (!IsPlayerConnected(targetid))return SendClientMessage(playerid, 0xFF0000FF, "! äàééãé ùáçøú ìà îçåáø");
	    if (playerid == targetid)return SendClientMessage(playerid, 0xFF0000FF, "! àéï àôùøåú ìùìåç äåãòä ìòöîê");
